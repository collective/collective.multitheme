<section  id="jarallax" data-jarallax="" style="background-image: none; z-index: 0; background-attachment:
    scroll; background-size: auto;  height: 50px" data-jarallax-original-styles="background-image: url(${view/data/image_url});">
<div class="full-bg-img">
    <div class="container">
        <div class="col-md-10 offset-md-1">
            <h1>
                ${view/data/title}
            </h1>
            <h3>
                ${view/data/description}
            </h3>
            <p>
                ${view/data/text}
            </p>
            <div class="button_group">
                <a class="btn btn-primary" href="${view/data/url1}">
                    <span class="fa ${view/data/icon1}"></span>
                    ${view/data/button1}
                </a>
                <a class="btn btn-outline" href="${view/data/url2}">
                    <span class="fa ${view/data/icon2}"></span>
                    ${view/data/button2}
                </a>
            </div>
        </div>
    </div>
</div>
<div id="jarallax-container-0" style="position: absolute; top: 0px; left: 0px; width: 100%; height: 100%;
        overflow: hidden; pointer-events: none; visibility: hidden; z-index: -100;">
        <div style="background-size: 100%;
        background-image: url(${view/data/image_url}); position: fixed; top: 0px; left: 0px; width: 2629.03967611336px;
        height: 1591.6px; overflow: hidden; pointer-events: none; margin-left: -297.0198380566801px; margin-top: -609.3px;
        visibility: visible; transform: translateY(67.7px) translateZ(0px); background-position: 50% 50%; background-repeat: no-repeat no-repeat;">
    </div>
</div>
</section>
