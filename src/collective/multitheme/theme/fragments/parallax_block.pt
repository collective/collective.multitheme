<section class="header1 parallax-background" style="z-index: 0; position: relative; background-image: none; background-attachment: scroll; background-size: auto;">
    <div class="container">
        <div class="col-md-10 offset-md-1">
            <h1>
                ${view/data/title}
            </h1>
            <h3>
                ${view/data/description}
            </h3>
            <p>
                ${view/data/text}
            </p>
            <div class="button_group">
                <a class="btn btn-primary" href="${view/data/url1}">
                    <span class="mbr-iconfont"></span>
                    ${view/data/button1}
                </a>
                <a class="btn btn-outline" href="${view/data/url2}">
                    <span class="fa ${view/data/icon2}"></span>
                    ${view/data/button2}
                </a>
            </div>
        </div>
    </div>
</section>
